{% if include.data %}
  <section>
    {% if include.title %}<h2>{{ include.title }}</h2>{% endif %}
    {% for item in include.data %}
      <p>
        <i class="{{ item.icon }}"></i>
        {% if item.url %}
          <a href="{{ item.url }}" target="_blank">{{ item.text }}</a>
        {% else %}
          <span>{{ item.text }}</span>
        {% endif %}
      </p>
    {% endfor %}
  </section>
{% endif %}
